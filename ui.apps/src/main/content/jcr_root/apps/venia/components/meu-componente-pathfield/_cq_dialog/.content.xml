<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Meu Componente Path Field"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
               <href cq-msm-lockable="./href"
                     fieldLabel="Redirecionamento do Link"
                     jcr:primaryType="nt:unstructured"
                     name="./href"
                     rootPath="/content"
                     sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <isExternal cq-msm-lockable="./isExternal"
                           jcr:primaryType="nt:unstructured"
                           name="./isExternal"
                           sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                           text="Redirecionamento Externo?"
                           value="{Boolean}true"/>
               <isBlank cq-msm-lockable="./isBlank"
                        jcr:primaryType="nt:unstructured"
                        name="./isBlank"
                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                        text="Abrir em uma nova Aba/PÃ¡gina?"
                        value="{Boolean}true"/>
            </items>
         </column>
      </items>
   </content>
</jcr:root>
