{
  "project-settings": {
    "code-owner": "Venia",
    "bundle-path": "../core/src/main/java",
    "test-path": "../core/src/test/java",
    "apps-path": "../ui.apps/src/main/content/jcr_root/apps",
    "component-path": "venia",
    "model-interface-pkg": "com.venia.core.models.commerce",
    "model-impl-pkg": "com.venia.core.models.commerce"
  },
  "name": "meu-componente-radio",
  "title": "Meu Componente Radio",
  "group": "Venia - Content",
  "type": "components",
  "options": {
    "js": false,
    "jstxt": false,
    "css": false,
    "csstxt": false,
    "html": false,
    "html-content": false,
    "slingmodel": false,
    "testclass": false,
    "junit-major-version": 5,
    "content-exporter": false,
    "model-adaptables": [
      "request"
    ],
    "generic-javadoc": false,
    "properties": [
      {
        "field": "role",
        "type": "radiogroup",
        "label": "Selecione uma opção:",
        "json-expose": true,
        "attributes": {
          "vertical": "{Boolean}true"
        },
        "items": [
          {
            "field": "junior",
            "type": "radio",
            "attributes": {
              "checked": "{Boolean}true",
              "name": "role",
              "text": "Júnior",
              "value": "Júnior"
            }
          },
          {
            "field": "pleno",
            "type": "radio",
            "attributes": {
              "checked": "{Boolean}false",
              "name": "role",
              "text": "Pleno",
              "value": "Pleno"
            }
          },
          {
            "field": "senior",
            "type": "radio",
            "attributes": {
              "checked": "{Boolean}false",
              "name": "role",
              "text": "Sênior",
              "value": "Sênior"
            }
          }
        ]
      }
    ]
  }
}